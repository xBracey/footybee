    "@types/passport-local": "^1.0.33",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-redux": "^7.1.15",
    "@types/react-select": "^4.0.15",
    "@types/react-table": "^7.0.29",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.0.0",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
