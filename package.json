{
  "name": "footybee-predictor",
  "private": true,
  "description": "Footybee predictor",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up",
    "build:site": "next build",
    "develop:site": "node server NODE_ENV=development",
    "serve:site": "node server NODE_ENV=production",
    "start:api": "ts-node -r tsconfig-paths/register ./src/api/index.ts",
    "develop:api": "nodemon --exec ts-node -r tsconfig-paths/register ./src/api/index.ts",
    "develop": "concurrently \"npm run develop:site\" \"npm run develop:api\"",
    "serve": "npm run build:site && concurrently \"npm run serve:site\" \"npm run start:api\"",
    "test": "jest --collectCoverage",
    "test:watch": "jest --collectCoverage --watch",
    "storybook": "start-storybook -p 9000",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@storybook/react": "^6.0.22",
    "@svgr/webpack": "^5.4.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.16",
    "@types/node": "^13.13.5",
    "@types/react-select": "^3.0.13",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.0.0",
    "babel-loader": "^8.2.1",
    "bcrypt": "^5.0.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "jest": "^26.0.1",
    "mysql2": "^2.1.0",
    "next": "^10.0.3",
    "nodemon": "^2.0.3",
    "react": "^16.12.0",
    "react-docgen-typescript": "^1.20.5",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-select": "^3.1.0",
    "react-use-dimensions": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "sequelize": "^5.21.8",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^4.2.0",
    "styled-components": "^5.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeface-poppins": "^0.0.72",
    "typeface-roboto": "^0.0.75",
    "typeface-salsa": "^0.0.72",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.1",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  }
}
