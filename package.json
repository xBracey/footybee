{
  "name": "footybee-predictor",
  "private": true,
  "description": "Footybee predictor",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up",
    "build:site": "next build",
    "develop:site": "NODE_ENV=development ts-node -r tsconfig-paths/register server/index.ts",
    "forever:develop:site": "NODE_ENV=development forever start -c \"ts-node -r tsconfig-paths/register\" server/index.ts",
    "develop": "nodemon --watch \"src/api/**\" --ext \"ts,json\" --exec \"yarn develop:site\"",
    "serve": "NODE_ENV=production forever start -c \"ts-node -r tsconfig-paths/register\" server/index.ts",
    "forever:stop": "forever stopall",
    "forever:list": "forever list",
    "test": "jest --collectCoverage",
    "test:watch": "jest --collectCoverage --watchAll",
    "storybook": "start-storybook -s ./public -p 9000 ",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "clean": "gatsby clean",
    "deploy": "docker-compose down && git pull && yarn && yarn build:site && yarn start -d",
    "deploy:soft": "docker-compose stop && git pull && yarn && yarn build:site && yarn start -d"
  },
  "dependencies": {
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-knobs": "^6.1.14",
